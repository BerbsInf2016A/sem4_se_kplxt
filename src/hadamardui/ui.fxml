      <TextField fx:id="dimension" layoutX="79.0" layoutY="2.0" prefHeight="25.0" prefWidth="82.0" />
      <Button fx:id="startButton" layoutX="178.0" layoutY="2.0" mnemonicParsing="false" onAction="#startButtonClicked" text="Start" />
