      <ChoiceBox fx:id="strategyChoiceBox" layoutX="300.0" layoutY="2.0" onAction="#choiceMade" prefWidth="150.0" value="Sylvester">
       <items>
           <FXCollections fx:factory="observableArrayList">
               <String fx:value="Sylvester" />
